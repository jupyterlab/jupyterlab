   "source": [
    "def com_avg(nums):\n",
    "    t = 0\n",
    "    for n in nums:\n",
    "        t += n\n",
    "    return t / len(nums)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "b4dab643-b043-4b79-806f-5fc9a6e58f02",
   "metadata": {},
   "outputs": [],
   "source": [
    "nums = [10, 20, 40]\n",
    "r = com_avg(nums)\n",
    "print('Avg', r)"
   ]
