    "@phosphor/signaling": "^1.2.2",
    "ajv": "^6.5.5",
    "json5": "^2.1.0",
    "minimist": "~1.2.0",
    "moment": "^2.24.0",
    "path-posix": "~1.0.0",
    "url-parse": "~1.4.3"
  },
  "devDependencies": {
    "@types/json5": "^0.0.30",
    "@types/minimist": "^1.2.0",
    "rimraf": "~2.6.2",
    "typedoc": "^0.14.2",
    "typescript": "~3.5.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "coreDependency": true
