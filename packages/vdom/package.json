{
  "name": "@jupyterlab/vdom",
  "version": "3.1.0-alpha.3",
  "description": "A viewer for VDOM documents.",
  "homepage": "https://github.com/jupyterlab/jupyterlab",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "sideEffects": [
    "style/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "style/*"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/apputils": "^3.1.0-alpha.3",
    "@jupyterlab/docregistry": "^3.1.0-alpha.3",
    "@jupyterlab/rendermime-interfaces": "^3.1.0-alpha.3",
    "@jupyterlab/services": "^6.1.0-alpha.3",
    "@lumino/coreutils": "^1.5.3",
    "@lumino/messaging": "^1.4.3",
    "@lumino/widgets": "^1.16.1",
    "@nteract/transform-vdom": "^4.0.16-alpha.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "rimraf": "~3.0.0",
    "typedoc": "~0.20.0-beta.27",
    "typescript": "~4.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "styleModule": "style/index.js"
}
