{
  "name": "@jupyterlab/services-extension",
  "version": "4.6.0-alpha.2",
  "description": "JupyterLab - Services Extension",
  "homepage": "https://github.com/jupyterlab/jupyterlab",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "sideEffects": [
    "style/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/*.json",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "src/**/*.{ts,tsx}",
    "style/index.js"
  ],
  "scripts": {
    "build": "tsc -b",
    "build:tsgo": "tsgo -b",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/services": "^7.6.0-alpha.2",
    "@lumino/coreutils": "^2.2.2"
  },
  "devDependencies": {
    "@jupyterlab/testing": "^4.6.0-alpha.2",
    "@types/jest": "^29.2.0",
    "@typescript/native-preview": "7.0.0-dev.20260128.1",
    "rimraf": "~5.0.5",
    "typescript": "~5.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true
  }
}
