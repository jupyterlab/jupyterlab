{
  "name": "@jupyterlab/toc",
  "version": "5.0.0-alpha.10",
  "description": "JupyterLab - Table of Contents widget",
  "keywords": [
    "jupyterlab"
  ],
  "homepage": "https://github.com/jupyterlab/jupyterlab",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/*.json",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "sideEffects": [
    "style/*.css"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "scripts": {
    "build": "tsc -b",
    "build:test": "tsc --build tsconfig.test.json",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:cov": "jest --collect-coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:debug:watch": "node --inspect-brk node_modules/.bin/jest --runInBand --watch",
    "watch": "tsc -b --watch"
  },
  "lint-staged": {
    "**/*{.ts,.tsx,.css,.json,.md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@jupyterlab/apputils": "^3.0.0-alpha.10",
    "@jupyterlab/cells": "^3.0.0-alpha.10",
    "@jupyterlab/coreutils": "^5.0.0-alpha.10",
    "@jupyterlab/docmanager": "^3.0.0-alpha.10",
    "@jupyterlab/docregistry": "^3.0.0-alpha.10",
    "@jupyterlab/fileeditor": "^3.0.0-alpha.10",
    "@jupyterlab/markdownviewer": "^3.0.0-alpha.10",
    "@jupyterlab/notebook": "^3.0.0-alpha.10",
    "@jupyterlab/rendermime": "^3.0.0-alpha.10",
    "@jupyterlab/translation": "^3.0.0-alpha.10",
    "@jupyterlab/ui-components": "^3.0.0-alpha.10",
    "@lumino/coreutils": "^1.4.3",
    "@lumino/messaging": "^1.3.3",
    "@lumino/signaling": "^1.3.5",
    "@lumino/widgets": "^1.11.1",
    "react": "~16.9.0",
    "react-dom": "~16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@jupyterlab/testutils": "^3.0.0-alpha.10",
    "@types/jest": "^24.0.23",
    "@types/react": "~16.9.16",
    "@types/react-dom": "~16.9.4",
    "jest": "^25.2.3",
    "lint-staged": "^8.2.1",
    "prettier": "^1.19.1",
    "rimraf": "~3.0.0",
    "ts-jest": "^25.2.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "~3.9.2"
  },
  "resolutions": {
    "@types/react": "~16.4.13"
  },
  "publishConfig": {
    "access": "public"
  }
}
