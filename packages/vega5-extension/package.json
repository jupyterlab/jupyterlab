{
  "name": "@jupyterlab/vega5-extension",
  "version": "2.0.0-alpha.2",
  "description": "JupyterLab - Vega 5 and Vega-Lite 3 Mime Renderer Extension",
  "homepage": "https://github.com/jupyterlab/jupyterlab",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "style/*.*"
  ],
  "sideEffects": [
    "style/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc -b && cd ../../prebuild/vega5 && jlpm && jlpm run build",
    "clean": "rimraf lib",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/rendermime-interfaces": "^2.0.0-alpha.2",
    "@lumino/coreutils": "^1.3.1",
    "@lumino/widgets": "^1.9.0"
  },
  "devDependencies": {
    "@types/webpack-env": "^1.13.9",
    "rimraf": "~2.6.2",
    "typedoc": "^0.15.2",
    "typescript": "~3.7.2",
    "vega": "^5.4.0",
    "vega-embed": "^4.2.0",
    "vega-lite": "^3.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "mimeExtension": true
  }
}
