  "dependencies": {
    "@jupyterlab/application": "^3.0.0-alpha.8",
    "@jupyterlab/apputils": "^3.0.0-alpha.8",
    "@lumino/coreutils": "^1.4.3",
    "@lumino/disposable": "^1.3.5",
    "@lumino/signaling": "^1.3.5",
    "@lumino/widgets": "^1.11.1",
    "react": "~16.9.0"
  },
  "devDependencies": {
    "rimraf": "~3.0.0",
    "typescript": "~3.9.2"
