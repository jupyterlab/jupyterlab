{
  "name": "@jupyterlab/hub-extension",
  "version": "3.1.0-alpha.3",
  "description": "JupyterLab integration for JupyterHub",
  "homepage": "https://github.com/jupyterlab/jupyterlab",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "sideEffects": [
    "style/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/*.json",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/index.js"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "prepublishOnly": "npm run build",
    "watch": "tsc -w --listEmittedFiles"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.1.0-alpha.3",
    "@jupyterlab/apputils": "^3.1.0-alpha.3",
    "@jupyterlab/coreutils": "^5.1.0-alpha.3",
    "@jupyterlab/mainmenu": "^3.1.0-alpha.3",
    "@jupyterlab/services": "^6.1.0-alpha.3",
    "@jupyterlab/translation": "^3.1.0-alpha.3"
  },
  "devDependencies": {
    "rimraf": "~3.0.0",
    "typescript": "~4.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true
  },
  "styleModule": "style/index.js"
}
