    "@jupyterlab/rendermime": "^4.6.0-alpha.2",
    "@jupyterlab/toc": "^6.6.0-alpha.2",
    "@jupyterlab/translation": "^4.6.0-alpha.2",
    "@lumino/coreutils": "^2.2.2",
    "@lumino/messaging": "^2.0.4",
    "@lumino/signaling": "^2.1.5",
