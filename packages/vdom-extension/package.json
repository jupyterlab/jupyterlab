{
  "name": "@jupyterlab/vdom-extension",
  "version": "3.0.0-beta.1",
  "description": "JupyterLab - VDOM Renderer",
  "homepage": "https://github.com/jupyterlab/jupyterlab",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "sideEffects": [
    "style/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "style/*"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.0-beta.1",
    "@jupyterlab/apputils": "^3.0.0-beta.1",
    "@jupyterlab/docregistry": "^3.0.0-beta.1",
    "@jupyterlab/notebook": "^3.0.0-beta.1",
    "@jupyterlab/rendermime": "^3.0.0-beta.1",
    "@jupyterlab/ui-components": "^3.0.0-beta.1",
    "@jupyterlab/vdom": "^3.0.0-beta.1"
  },
  "devDependencies": {
    "rimraf": "~3.0.0",
    "typedoc": "0.17.0-3",
    "typescript": "~4.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true
  }
}
