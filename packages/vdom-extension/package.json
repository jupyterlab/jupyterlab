{
  "name": "@jupyterlab/vdom-extension",
  "version": "1.0.0-alpha.7",
  "description": "JupyterLab - VDOM Renderer",
  "homepage": "https://github.com/jupyterlab/jupyterlab",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "style/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib",
    "docs": "typedoc --options tdoptions.json --theme ../../typedoc-theme src",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0-alpha.7",
    "@jupyterlab/apputils": "^1.0.0-alpha.7",
    "@jupyterlab/docregistry": "^1.0.0-alpha.7",
    "@jupyterlab/notebook": "^1.0.0-alpha.8",
    "@jupyterlab/rendermime": "^1.0.0-alpha.7",
    "@jupyterlab/vdom": "^1.0.0-alpha.7"
  },
  "devDependencies": {
    "rimraf": "~2.6.2",
    "typedoc": "^0.14.2",
    "typescript": "~3.4.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true
  }
}
