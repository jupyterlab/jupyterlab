{
  "compilerOptions": {
    "declaration": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES5",
    "outDir": "./lib",
    "lib": [
      "ES5",
      "ES2015.Collection",
      "ES2015.Promise",
      "DOM"
    ],
    "baseUrl": ".",
    "paths": {
      "@jupyterlab/*": [
        "../*/src"
      ]
    },
    "jsx": "react",
    "jsxFactory": "h"
  }
}
