{
  "name": "@jupyterlab/codemirror-extension",
  "version": "0.8.3",
  "description": "JupyterLab - CodeMirror Provider Extension",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "schema/*.json"
  ],
  "directories": {
    "lib": "lib/"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.8.3",
    "@jupyterlab/apputils": "^0.8.2",
    "@jupyterlab/coreutils": "^0.8.1",
    "@jupyterlab/codeeditor": "^0.8.1",
    "@jupyterlab/codemirror": "^0.8.1",
    "@jupyterlab/fileeditor": "^0.8.2",
    "@phosphor/widgets": "^1.3.0"
  },
  "devDependencies": {
    "rimraf": "^2.5.2",
    "typescript": "~2.4.1"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w"
  },
  "jupyterlab": {
    "extension": true,
    "schemas": [
      "schema/jupyter.services.codemirror-commands.json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab.git"
  },
  "author": "Project Jupyter",
