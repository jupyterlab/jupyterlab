{
  "name": "@jupyterlab/codemirror-extension",
  "version": "3.0.0-beta.1",
  "description": "JupyterLab - CodeMirror Provider Extension",
  "homepage": "https://github.com/jupyterlab/jupyterlab",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "sideEffects": [
    "style/**/*.css"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "schema/*.json",
    "style/**/*.css"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.0-beta.1",
    "@jupyterlab/codeeditor": "^3.0.0-beta.1",
    "@jupyterlab/codemirror": "^3.0.0-beta.1",
    "@jupyterlab/docregistry": "^3.0.0-beta.1",
    "@jupyterlab/fileeditor": "^3.0.0-beta.1",
    "@jupyterlab/mainmenu": "^3.0.0-beta.1",
    "@jupyterlab/settingregistry": "^3.0.0-beta.1",
    "@jupyterlab/statusbar": "^3.0.0-beta.1",
    "@jupyterlab/translation": "^3.0.0-beta.1",
    "@lumino/widgets": "^1.14.0",
    "codemirror": "~5.57.0"
  },
  "devDependencies": {
    "rimraf": "~3.0.0",
    "typedoc": "0.17.0-3",
    "typescript": "~4.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
}
