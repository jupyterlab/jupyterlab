    "mini-css-extract-plugin": "~0.8.0",
    "package-json": "^6.5.0",
    "path": "~0.12.7",
    "prettier": "^1.19.1",
    "semver": "^6.3.0",
    "sort-package-json": "~1.31.0",
    "typescript": "~3.7.3",
    "webpack": "^4.41.2",
    "which": "^2.0.2"
