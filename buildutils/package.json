  "devDependencies": {
    "@types/duplicate-package-checker-webpack-plugin": "^2.1.0",
    "@types/fs-extra": "^8.0.1",
    "@types/glob": "^7.1.1",
    "@types/inquirer": "^6.5.0",
    "@types/mini-css-extract-plugin": "^0.8.0",
    "@types/node": "^12.12.17",
    "@types/prettier": "^1.19.0",
    "@types/webpack": "^4.41.0",
    "@types/which": "^1.3.2",
    "rimraf": "~3.0.0"
  },
  "publishConfig": {
    "access": "public"
