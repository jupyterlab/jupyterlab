    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build",
    "watch": "tsc -w --listEmittedFiles"
  },
  "dependencies": {
    "@lumino/coreutils": "^1.4.2",
    "@yarnpkg/lockfile": "^1.1.0",
    "child_process": "~1.0.2",
    "commander": "~4.0.1",
    "crypto": "~1.0.1",
    "dependency-graph": "^0.8.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "fs-extra": "^8.1.0",
    "glob": "~7.1.6",
